<div class="header">
  
  <div class="D.maker">
    <%= link_to(root_path, class: 'D.maker-btn') do %>
      <%= image_tag 'logo1.png', size: '200x100', class: "D.maker-btn-icon" %>
    <% end %>
  </div>

  <div class="button">
    <div class="login">
      <%= link_to("#", class: 'sign_in-btn') do %>
        <%= image_tag 'login.png', size: '80x80', class: "sign_in-btn-icon" %>
      <% end %>
    </div>

    <div class="logout">
      <%= link_to("#", class: 'logout-btn') do %>
        <%= image_tag 'logout.png', size: '80x80', class: "logout-btn-icon" %>
      <% end %>
    </div>
  </div>

</div>

<div class="main"></div>

<div class="footer">
